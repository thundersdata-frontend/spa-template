{
  "name": "spa-template",
  "version": "1.0.0",
  "description": "杭州雷数前端中后台管理系统模板",
  "license": "MIT",
  "author": "陈杰 <chenjie@thundersdata.com>",
  "homepage": "https://github.com/thundersdata-frontend/spa-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundersdata-frontend/spa-template.git"
  },
  "bugs": {
    "url": "https://github.com/thundersdata-frontend/spa-template/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "umi dev",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env UMI_ENV=prod umi build",
    "test": "jest --passWithNoTests",
    "tsc": "tsc -p ./tsconfig.json",
    "eslint:fix": "eslint --fix --ext .ts,.tsx src/",
    "lint:fix": "prettier --check src/**/*.tsx --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "changelog": {
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run eslint:fix && npm run lint:fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@ant-design/pro-layout": "^5.0.9",
    "@umijs/preset-react": "1.x",
    "antd": "^4.1.4",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.25",
    "lodash": "^4.17.15",
    "lscache": "^1.3.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "umi": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.150",
    "@types/lscache": "^1.3.0",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "@umijs/fabric": "^2.0.8",
    "antd-dayjs-webpack-plugin": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.4",
    "conventional-changelog-cli": "^2.0.28",
    "conventional-changelog-custom-config": "^0.3.1",
    "cross-env": "^7.0.2",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "pont-engine": "1.0.3",
    "prettier": "^2.0.4",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
